{
  "manifest_version": 3,
  "name": "Audio Capture and Display",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabCapture",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}








  
  
  
  